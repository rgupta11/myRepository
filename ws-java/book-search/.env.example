# Datadog Configuration
# Replace with your actual Datadog API key from https://app.datadoghq.com/organization-settings/api-keys
DD_API_KEY=sample

# Datadog site (change if you're in EU, etc.)
# US: datadoghq.com, EU: datadoghq.eu, US3: us3.datadoghq.com, US5: us5.datadoghq.com
DD_SITE=datadoghq.com

# Environment name (development, staging, production, etc.)
DD_ENV=development

# Application version
DD_VERSION=1.0.0

# Database Configuration (existing)
POSTGRES_DB=library
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin

# Spring Boot Application Configuration
DB_URL=jdbc:postgresql://library-db:5432/library
DB_USER=admin
DB_PASSWORD=admin
